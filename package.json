{
  "name": "tsbean-driver-mysql",
  "version": "1.1.3",
  "description": "MySQL driver for TSBean-ORM. Works with any database with support for the MySQL protocol.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/AgustinSRG/tsbean-driver-mysql",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgustinSRG/tsbean-driver-mysql"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "compile": "tsc --pretty --declaration",
    "build": "rimraf dist && npm run compile && npm run gendoc",
    "lint": "eslint --fix src/**/*.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "gendoc": "typedoc --out docs ./src"
  },
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Agustin San Roman",
    "email": "agustinsanromanguzman@gmail.com",
    "url": "https://github.com/AgustinSRG"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "peerDependencies": {
    "tsbean-orm": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "license": "MIT",
  "dependencies": {
    "mysql2": "2.3.3",
    "tsbean-orm": "^1.1.1"
  }
}
